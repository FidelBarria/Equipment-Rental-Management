<h1>Categories</h1>

<%= link_to "New Category", new_category_path %>

<table class="table">
    <thead>
        <tr>
            <td>Name</td>
            <td>Description</td>
        </tr>
    </thead>
    <tbody>
        <% @categories.each do |category|%>
        <tr>
            <td><%= category.name%></td>
            <td><%= category.description%></td>
        </tr>
        <%end%>
    </tbody>
</table>



    