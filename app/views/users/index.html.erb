<h1>Users<h1>

<%= link_to "New User", signup_path%>

<table class="table">
    <thead>
        <tr>
            <td>Name</td>
            <td>User</td>
            <td>Email</td>
            <td>Password</td>
        </tr>
    </thead>  
    <tbody>
        <% @users.each do |user|%>
        <tr>
            <td><%=user.name%></td>
            <td><%=user.user%></td>
            <td><%=user.email%></td>
            <td><%=user.password%></td>
        </tr>
        <%end%>
    </tbody>  
</table>