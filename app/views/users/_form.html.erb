<%= form_with model: user do |form|%>
    <%= form.label :name%>
    <%= form.text_field :name%>

    <%= form.label :user%>
    <%= form.text_field :user%>

    <%= form.label :email%>
    <%= form.text_field :email%>

    <%= form.label :password%>
    <%= form.text_field :password%>

    <%= form.submit%>
<%end%>